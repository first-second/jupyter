   "source": [
    "def decorator(abc):\n",
    "    def wrapper():\n",
    "        print(\"inside decorator\")\n",
    "        return abc()\n",
    "    return wrapper\n",
    "\n",
    "@decorator\n",
    "def func():\n",
    "    ps=\"inside func\"\n",
    "    return ps\n",
    "func()"
